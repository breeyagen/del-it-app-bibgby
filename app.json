
{
  "expo": {
    "name": "Del It",
    "slug": "del-it",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/natively-dark.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/natively-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#F7F9FC"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.delit.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "Del It needs access to your photos to help you find and remove duplicates, WhatsApp images, and screenshots.",
        "NSPhotoLibraryAddUsageDescription": "Del It needs permission to save photos to your library."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/natively-dark.png",
        "backgroundColor": "#F7F9FC"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.delit.app",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO"
      ]
    },
    "web": {
      "favicon": "./assets/images/final_quest_240x240.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      [
        "expo-media-library",
        {
          "photosPermission": "Del It needs access to your photos to help you find and remove duplicates, WhatsApp images, and screenshots.",
          "savePhotosPermission": "Del It needs permission to manage your photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "scheme": "delit",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  }
}
